# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# OpenAI API
OPENAI_API_KEY=sk-your-openai-api-key

# SERP API (SerpApi or SearchAPI)
SERPAPI_KEY=your-serpapi-key

# Redis (for BullMQ)
REDIS_URL=redis://localhost:6379

# Payment Provider (Flutterwave example)
FLUTTERWAVE_PUBLIC_KEY=FLWPUBK_TEST-your-key
FLUTTERWAVE_SECRET_KEY=FLWSECK_TEST-your-secret
FLUTTERWAVE_WEBHOOK_SECRET=your-webhook-secret

# Application
NODE_ENV=development
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:4000

# n8n (optional)
N8N_WEBHOOK_URL=http://localhost:5678/webhook

# Database (if using local Postgres instead of Supabase)
# DATABASE_URL=postgresql://user:password@localhost:5432/compet_int
